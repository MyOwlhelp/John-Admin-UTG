 do
 v.CanCollide = false
 
 wait(0)
 local player = game.Players.LocalPlayer
 local mouse = player:GetMouse()
 bind = "e" -- has to be lowercase
 mouse.KeyDown:connect(function(key)
 if key == bind then do
 v.CanCollide = true
 end
 end
 end)
 end
 end
 end
