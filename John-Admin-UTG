	 Spin.Parent = getRoot(game.Players.LocalPlayer.Character)
	 Spin.MaxTorque = Vector3.new(0, math.huge, 0)
	 Spin.AngularVelocity = Vector3.new(0,40,0)
		 until Looptornado == false
	 end)
 
