 -- Variables
 local mouse = game.Players.LocalPlayer:GetMouse()
 local uis = game:GetService("UserInputService")
 
 -- Connect
 mouse.Button1Down:Connect(function()
	if mouse.Target and uis:IsKeyDown(Enum.KeyCode.LeftControl) then
