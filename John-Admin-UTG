		 attachTool(tool,CFrame.new(0,0,0.2) * CFrame.Angles(math.rad(-90),0,0))
		 tool.Grip = plrtorso.CFrame
		 wait(0.07)
 tool.Grip = CFrame.new(0, -7, -3)
