wait();
Notify({
Description = "Anti fling disabled";
Title = "Nameless Admin";
Duration = 5;

});

for _,v in pairs(game.Players:GetPlayers()) do
	if v ~= game.Players then
char = v.Character
for _,x in pairs(char:GetDescendants()) do
	if x:IsA("BasePart") then
		x.CanCollide = true
	end
end
	end
end
			 end)
 
